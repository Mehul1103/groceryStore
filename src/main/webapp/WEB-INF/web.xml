<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SimpleServlet</servlet-name>
		<servlet-class>SimpleServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ExecuteDbQueryServlet</servlet-name>
		<display-name>ExecuteDbQueryServlet</display-name>
		<description></description>
		<servlet-class>com.grocery.servlets.ExecuteDbQueryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SimpleServlet</servlet-name>
		<url-pattern>/SimpleServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExecuteDbQueryServlet</servlet-name>
		<url-pattern>/ExecuteDbQueryServlet</url-pattern>
	</servlet-mapping>
</web-app>
